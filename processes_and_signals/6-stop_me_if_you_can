#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond.sh process without using kill or killall.

pid=$(pgrep -o -f "4-to_infinity_and_beyond.sh")  # Get the oldest process ID

if [[ -n "$pid" ]]; then
    echo "Stopping process $pid..."
    echo "" > "/proc/$pid/cmdline"  # Overwrite cmdline to disrupt execution
fi
